    <section class="sort">
        <div class="sort_bar">
            <h4>Home / Doctor List</h4>
            <form method="POST" action="/sort_doctors">
                <select class="dropbtn" name="sort_by" id="sort_by" onchange="this.form.submit()">
                    <option value hidden <% if(sort_by == "") {%>selected<% } %> disabled>Sort by <i
                            class="fas fa-chevron-down"></i></option>
                    <option <% if(sort_by == "name") {%>selected<% } %> value="name">Name(ascending)</option>
                    <option <% if(sort_by == "-name") {%>selected<% } %> value="-name">Name(descending)</option>
                    <option <% if(sort_by == "fee") {%>selected<% } %> value="fee">Fee(ascending)</option>
                    <option <% if(sort_by == "-fee") {%>selected<% } %> value="-fee">Fee(descending)</option>
                    <option <% if(sort_by == "experience") {%>selected<% } %> value="experience">Experience(ascending)
                    </option>
                    <option <% if(sort_by == "-experience") {%>selected<% } %>value="-experience">Experience(descending)
                    </option>
                </select>
            </form>

        </div>
    </section>

